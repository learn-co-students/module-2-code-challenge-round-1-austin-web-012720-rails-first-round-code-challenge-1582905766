<%= form_for @heroine do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :super_name %>
    <%= f.text_field :super_name %>

    <%= f.collection_select :heroine_power_ids, Power.all, :id, :name, prompt: true  %>

    <%= f.submit %>
    <%= @heroine.errors.full_messages.first if @heroine.errors.any? %>
<% end %>


