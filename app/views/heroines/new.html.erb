<h1>Create a New Heroine: </h1>
<%= form_for(@heroine) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :super_name %>
<%= f.text_field :super_name %>
<br>
<%= collection_select(:power, :heroine_id, Power.all, :id, :name) %>
<br>
<%= f.submit%>
<br>
<h1>
<%= @heroine.errors.full_messages.first if @heroine.errors.any? %>
</h1>
<% end %>