<h1>Create a New Heroine: </h1>
<%= form_for(@heroine) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :super_name %>
<%= f.text_field :super_name %>
<br>
<%= f.collection_select(:power, Heroine.all, :id, :name) %>

<%= f.submit%>
<br>
<h1>
<%= @heroine.errors.full_messages.first if @heroine.errors.any? %>
</h1>
<% end %>